# Supabase - Database & Authentication
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe - Payment Processing
# For test mode, use a key that starts with pk_test_
# For production mode, use a key that starts with pk_live_
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# TicketNetwork API Configuration
# If these values are missing or set to placeholder values, the app will use mock data
VITE_TICKETNETWORK_API_KEY=your-ticketnetwork-api-key   # Replace with your TicketNetwork API key
VITE_TICKETNETWORK_CONSUMER_KEY=your-consumer-key       # OAuth consumer key
VITE_TICKETNETWORK_CONSUMER_SECRET=your-consumer-secret # OAuth consumer secret
VITE_TICKETNETWORK_ENVIRONMENT=sandbox                  # 'sandbox' or 'production'
VITE_TICKETNETWORK_WCID=your-wcid                       # Website Config ID (use either WCID or BID)
VITE_TICKETNETWORK_BID=your-bid                         # Broker ID (use either WCID or BID)

# TicketVault API Configuration (for electronic ticket delivery)
VITE_TICKETVAULT_API_KEY=your-ticketvault-api-key
VITE_TICKETVAULT_API_SECRET=your-ticketvault-api-secret
VITE_TICKETVAULT_ENVIRONMENT=sandbox                    # 'sandbox' or 'production'

# Webhook Configuration
VITE_WEBHOOK_SECRET=your-webhook-signing-secret         # For verifying webhook signatures

# DevPortal API Configuration
VITE_DEVPORTAL_API_URL=https://api.devportal.example.com
VITE_DEVPORTAL_API_KEY=your-devportal-api-key
VITE_DEVPORTAL_CLIENT_ID=your-client-id
VITE_DEVPORTAL_CLIENT_SECRET=your-client-secret
